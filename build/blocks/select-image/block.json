{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "meloniq/kenyan-beads-block-select-image",
  "version": "1.0.0",
  "title": "Kenyan Beads Pattern Generator Block",
  "category": "media",
  "icon": "embed-photo",
  "description": "Display beads pattern for given image.",
  "supports": {
    "html": true
  },
  "textdomain": "kenyan-beads",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "attachmentId": {
      "type": "number",
      "default": 0
    },
    "colorPalette": {
      "type": "string",
      "default": "full"
    },
    "paddingSize": {
      "type": "object",
      "default": {
        "top": "0px",
        "bottom": "0px",
        "left": "5px",
        "right": "5px"
      }
    },
    "marginSize": {
      "type": "object",
      "default": {
        "top": "0px",
        "bottom": "0px",
        "left": "5px",
        "right": "5px"
      }
    }
  }
}